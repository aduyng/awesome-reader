{{#each tickets}}
<a href="#ticket/view/id/{{this.id}}" type="button" class="btn btn-list-item {{this.state.buttonClass}} btn-block btn-lg clearfix">
        <span class="pull-right right">{{formatMoney this.amount}}</span>
        T{{this.id}} - {{this.technician.name}}
    </a>
{{else}}
<p class="alert alert-warning">{{$ thereIsNoTicketsAddedYet}}</p>
{{/each}}