<div class="panel panel-default">
    <div class="panel-heading">
        <div class="pull-right">
            <input type="checkbox" id="{{id}}-lock-switch" data-size="mini" data-on-text="<i class='fa fa-unlock'></i>" data-off-text="<i class='fa fa-lock'></i>" />
        </div>
        <h3 class="panel-title">{{$ feeds}}</h3>
    </div>
    <div class="panel-body">
        <div id="{{id}}-list"></div>
    </div>
        
    <div class="panel-footer hidden" id="{{id}}-footer">
        <button class="btn btn-success btn-xs" id="{{id}}-new"><i class="fa fa-plus"></i> {{$ new}}</button>
    </div>
</div>

